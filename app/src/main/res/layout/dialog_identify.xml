<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <View
        android:id="@+id/view25"
        android:layout_width="@dimen/dp_640"
        android:layout_height="397dp"
        android:layout_centerInParent="true"
        android:background="@drawable/setting_item_bg"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView63"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/Finish_task_tips"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_40"
        android:layout_marginTop="@dimen/dp_90"
        android:layout_marginBottom="@dimen/dp_100"
        app:layout_constraintEnd_toEndOf="@+id/view25"
        app:layout_constraintStart_toStartOf="@+id/view25"
        app:layout_constraintTop_toTopOf="@+id/view25" />

    <EditText
        android:id="@+id/ed"
        android:layout_width="@dimen/dp_400"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_40"
        android:hint="请输入录入名字"
        app:layout_constraintEnd_toEndOf="@+id/textView63"
        app:layout_constraintStart_toStartOf="@+id/textView63"
        app:layout_constraintTop_toBottomOf="@+id/textView63" />


    <Button
        android:id="@+id/cancelBtn1"
        android:layout_width="@dimen/dp_204"
        android:layout_height="@dimen/dp_72"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="@dimen/dp_100"
        android:background="@drawable/dialog_btn_false_bg"
        android:padding="@dimen/dp_10"
        android:text="@string/cancel1"
        android:layout_marginTop="@dimen/dp_100"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_24"
        app:layout_constraintStart_toStartOf="@+id/view25"
        app:layout_constraintTop_toBottomOf="@+id/textView63" />

    <Button
        android:id="@+id/submitBtn1"
        android:layout_width="@dimen/dp_204"
        android:layout_height="@dimen/dp_72"
        android:layout_marginEnd="@dimen/dp_100"
        android:layout_marginTop="@dimen/dp_100"
        android:background="@drawable/dialog_btn_true_bg"
        android:text="@string/sure"
        android:textColor="@color/white"
        android:textSize="@dimen/sp_24"
        app:layout_constraintEnd_toEndOf="@+id/view25"
        app:layout_constraintTop_toBottomOf="@+id/textView63" />

</androidx.constraintlayout.widget.ConstraintLayout>