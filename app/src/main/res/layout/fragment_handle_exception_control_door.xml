<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background3"
        tools:context=".view.fragment.HandleExceptionRelocationFragment">

        <com.sendi.deliveredrobot.view.widget.CustomKeyBoardView
            android:id="@+id/customKeyBoardView"
            android:layout_width="416dp"
            android:layout_height="465dp"
            android:layout_marginBottom="69dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0" />

        <com.sendi.deliveredrobot.view.widget.CustomIndicatorTextView
            android:id="@+id/customIndicatorTextView"
            android:layout_width="198dp"
            android:layout_height="55dp"
            android:layout_marginTop="165dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:text_size="36" />

        <TextView
            android:id="@+id/textViewInputPassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="108dp"
            android:text="@string/please_input_password_chinese"
            android:textColor="@color/color_A0BAEF"
            android:textSize="@dimen/small_small_text_size"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <androidx.constraintlayout.widget.Group
            android:id="@+id/groupInputPassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            app:constraint_referenced_ids="customKeyBoardView,customIndicatorTextView,textViewInputPassword"
            tools:visibility="gone" />

        <View
            android:id="@+id/viewBin1"
            android:layout_width="632dp"
            android:layout_height="294dp"
            android:background="@drawable/shape_solid_f6f6f6_corner_10dp"
            app:layout_constraintBottom_toTopOf="@+id/viewBin2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageViewBin1"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="43dp"
            android:layout_marginTop="64dp"
            android:contentDescription="@string/image_description"
            android:src="@drawable/ic_box_2"
            app:layout_constraintBottom_toBottomOf="@+id/viewBin1"
            app:layout_constraintEnd_toEndOf="@+id/viewBin1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/viewBin1"
            app:layout_constraintTop_toTopOf="@+id/viewBin1"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textViewPlace1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="19dp"
            android:textColor="@color/color_4D6FBE"
            android:textSize="@dimen/mid_text_size"
            app:layout_constraintBottom_toBottomOf="@+id/imageViewBin1"
            app:layout_constraintEnd_toEndOf="@+id/viewBin1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageViewBin1"
            app:layout_constraintTop_toTopOf="@+id/imageViewBin1"
            android:text="@string/bin_1" />

        <View
            android:id="@+id/viewDivision1"
            android:layout_width="550dp"
            android:layout_height="1dp"
            android:layout_marginTop="128dp"
            android:background="@color/color_CFE2F5"
            app:layout_constraintBottom_toBottomOf="@+id/viewBin1"
            app:layout_constraintEnd_toEndOf="@+id/viewBin1"
            app:layout_constraintStart_toStartOf="@+id/viewBin1"
            app:layout_constraintTop_toTopOf="@+id/viewBin1"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textViewBinControlCommit1"
            android:layout_width="560dp"
            android:layout_height="80dp"
            android:layout_marginBottom="35dp"
            android:background="@drawable/selector_bg2_button"
            android:gravity="center"
            android:letterSpacing="@dimen/text_letter_space"
            tools:text="开仓"
            android:textColor="@color/white"
            android:textSize="@dimen/mid_text_size"
            app:layout_constraintBottom_toBottomOf="@+id/viewBin1"
            app:layout_constraintEnd_toEndOf="@+id/viewBin1"
            app:layout_constraintStart_toStartOf="@+id/viewBin1"
            app:layout_constraintTop_toTopOf="@+id/viewBin1"
            app:layout_constraintVertical_bias="1.0"
            tools:targetApi="lollipop" />

